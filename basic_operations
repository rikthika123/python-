# addition
a=int(input("enter the value of a"))
b=int(input("enter the value of b"))
c=a+b
print("addition is:",c)

#subtraction
a=int(input("enter the value of a"))
b=int(input("enter the value of b"))
c=a-b
print("subtraction is:",c)

#multiplication
a=int(input("enter the value of a"))
b=int(input("enter the value of b"))
c=a*b
print("multiplication is:",c)

#square
s=int(input("enter the side"))
print("area of square:",s*s)

