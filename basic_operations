area=int(input("enter the area of square:"))
print("area of square is:",6*a*a)

#(a+b)^2    #(a-b)^2   #(a+b)^3    #(a-b)^3  
a=int(input("enter the a "))
b=int(input("enter the b"))
print("result is:",(a+b)(a+b))

#area of circle   #square,triangle,rectangle,
rad=int(input("enter the radius:"))
 print("area is",3.14*rad*rad)

